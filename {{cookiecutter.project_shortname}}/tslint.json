{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "rules": {
        "member-access": false,
        "member-ordering": false,
        "quotemark": [true, "single"],
        "trailing-comma": [true, {
            "singleline": "never",
            "multiline": "never"
        }]
    },
    "rulesDirectory": []
}